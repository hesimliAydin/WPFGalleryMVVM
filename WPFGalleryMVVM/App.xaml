             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFGalleryMVVM"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictonaries/ButtonSize.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictonaries/Style.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictonaries/AutoPlay.xaml"></ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
